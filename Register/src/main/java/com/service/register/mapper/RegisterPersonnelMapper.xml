<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.service.register.mapper.RegisterPersonnelMapper">
    <!-- 查询所有网站信息 -->
    <select id="selectProcessToEmail"
            resultMap="registerResMap">
        select status,name,register_id,email from register_personnel where email =#{email};
    </select>
    <resultMap type="com.service.register.entity.RegisterPersonnel" id="registerResMap">
        <!-- 指定主键列的定义规则 -->
        <!-- property所指的id是javaBean Car中的id -->
        <!-- column所指的id是 mySql数据表中的id-->
        <id property="id" column="id"/>
        <!-- 普通列 -->
        <result property="status" column="status"/>
        <result property="name" column="name"/>
        <result property="registerId" column="register_id"/>
        <result property="email" column="email"/>
    </resultMap>
</mapper>
